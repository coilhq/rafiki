meta {
  name: Unrevoke Wallet Address Key
  type: graphql
  seq: 54
}

post {
  url: {{RafikiGraphqlHost}}/graphql
  body: graphql
  auth: none
}

body:graphql {
  mutation UnrevokeWalletAddressKey ($input: RevokeWalletAddressKeyInput!) {
      unrevokeWalletAddressKey (input: $input) {
          walletAddressKey {
              id
              revoked
              walletAddressId
              createdAt
          }
      }
  }
}

body:graphql:vars {
  {
      "input": {
          "id": "{{walletAddressKeyId}}"
      }
  }
}

script:pre-request {
  const scripts = require('./scripts');
  
  scripts.addApiSignatureHeader();
}
